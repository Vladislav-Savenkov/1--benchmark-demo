					<tl>
						<v8:item>
							<v8:lang>ru</v8:lang>
							<v8:content>1. Предмет договора</v8:content>
						</v8:item>
					</tl>
				</c>
			</c>
		</row>
	</rowsItem>
	<rowsItem>
		<index>7</index>
