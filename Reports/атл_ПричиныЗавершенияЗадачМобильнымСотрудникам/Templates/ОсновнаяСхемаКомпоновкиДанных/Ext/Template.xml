		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	атл_ЗадачаМобильномуСотруднику.ПричинаЗавершенияЗадачи,
	атл_ЗадачаМобильномуСотруднику.Ссылка КАК ЗадачаМобильномуСотруднику,
	атл_ЗадачаМобильномуСотруднику.ТекущийСтатус,
	атл_ЗадачаМобильномуСотруднику.Исполнитель,
	атл_ЗадачаМобильномуСотруднику.Контрагент,
	СУММА(1) КАК Количество,
	ЕСТЬNULL(ВЫБОР
			КОГДА &amp;ДатаНачала = &amp;ДатаКонца
				ТОГДА 1
			ИНАЧЕ РАЗНОСТЬДАТ(&amp;ДатаНачала, КОНЕЦПЕРИОДА(&amp;ДатаКонца, ДЕНЬ), ДЕНЬ)
		КОНЕЦ, 1) КАК КоличествоДней
ПОМЕСТИТЬ ИсходнаяТаблица
ИЗ
	Документ.атл_ЗадачаМобильномуСотруднику КАК атл_ЗадачаМобильномуСотруднику
ГДЕ
	атл_ЗадачаМобильномуСотруднику.Проведен = ИСТИНА
	И атл_ЗадачаМобильномуСотруднику.ЗавершилДата &lt;&gt; ДАТАВРЕМЯ(1, 1, 1)
	И атл_ЗадачаМобильномуСотруднику.ЗавершилДата МЕЖДУ &amp;ДатаНачала И КОНЕЦПЕРИОДА(&amp;ДатаКонца, ДЕНЬ)

СГРУППИРОВАТЬ ПО
	атл_ЗадачаМобильномуСотруднику.ПричинаЗавершенияЗадачи,
	атл_ЗадачаМобильномуСотруднику.Ссылка,
	атл_ЗадачаМобильномуСотруднику.ТекущийСтатус,
	атл_ЗадачаМобильномуСотруднику.Исполнитель,
	атл_ЗадачаМобильномуСотруднику.Контрагент
;

