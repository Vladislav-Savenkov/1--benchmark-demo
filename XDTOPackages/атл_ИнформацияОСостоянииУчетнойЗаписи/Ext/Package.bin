<package xmlns="http://v8.1c.ru/8.1/xdto" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" targetNamespace="company_info_mwms">
	<objectType name="mobile_users" ordered="false">
		<property xmlns:d3p1="company_info_mwms" name="mobile_user" type="d3p1:mobile_user" lowerBound="0" upperBound="-1" nillable="true"/>
	</objectType>
	<objectType name="mobile_user" ordered="false">
		<property name="external_id" nillable="true">
			<typeDef xsi:type="ValueType" base="xs:string" variety="Atomic" maxLength="50"/>
		</property>
		<property name="login">
			<typeDef xsi:type="ValueType" base="xs:string" variety="Atomic" maxLength="100"/>
		</property>
		<property name="authorized">
			<typeDef xsi:type="ValueType" base="xs:int" variety="Atomic"/>
		</property>
	</objectType>
	<objectType name="active" ordered="false">
		<property name="from_datetime" lowerBound="0">
			<typeDef xsi:type="ValueType" base="xs:dateTime" variety="Atomic" itemType="xs:dateTime"/>
		</property>
		<property name="to_datetime" lowerBound="0">
			<typeDef xsi:type="ValueType" base="xs:dateTime" variety="Atomic" itemType="xs:dateTime"/>
		</property>
	</objectType>
	<objectType name="company_info" ordered="false">
		<property xmlns:d3p1="company_info_mwms" name="active" type="d3p1:active" lowerBound="0" nillable="true"/>
		<property name="licence_count">
			<typeDef xsi:type="ValueType" base="xs:int" variety="Atomic" totalDigits="10"/>
		</property>
		<property name="tariff" lowerBound="0">
			<typeDef xsi:type="ValueType" base="xs:string" variety="Atomic" maxLength="50"/>
		</property>
		<property xmlns:d3p1="company_info_mwms" name="mobile_users" type="d3p1:mobile_users" lowerBound="0" nillable="true"/>
	</objectType>
</package>